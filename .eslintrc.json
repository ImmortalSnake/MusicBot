{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2019
    },
    "rules": {
        "no-console": "off",
        "no-var": "error",
        "comma-style": "error",
        "comma-spacing": "error",
        "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 1, "maxBOF": 0 }],
        "no-empty-function": "error",
        "no-trailing-spaces": ["error"],
        "no-unused-vars": "warn",
        "indent": [
            "error",
            2
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "always"
        ]
    }
}